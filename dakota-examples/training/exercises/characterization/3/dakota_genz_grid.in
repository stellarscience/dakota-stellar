environment
  tabular_data
    tabular_data_file = 'genz_grid_tabular.dat'

method
  multidim_parameter_study
    partitions = 5

variables
  uniform_uncertain = 2
    lower_bounds    = 0.0   0.0
    upper_bounds    = 1.0   1.0
    descriptors     = 'x1'  'x2'

interface
  analysis_drivers = 'genz'
    analysis_components = 'cp3'
    direct

responses
  response_functions = 1
  descriptors      = 'f1'
  no_gradients
  no_hessians

