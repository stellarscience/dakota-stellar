method
  conmin_mfd

variables
	continuous_design = 2
	  initial_point   -1.0      1.0
	  lower_bounds    -2.0     -2.0
	  upper_bounds     2.0      2.0
	  descriptors      'x1'     'x2'

interface
  analysis_drivers = 'simulator_script.sh'
	  fork
  	  parameters_file = 'params.in'
	    results_file    = 'results.out'

responses
	objective_functions = 1
 	numerical_gradients
	  fd_step_size = .000001
	no_hessians
