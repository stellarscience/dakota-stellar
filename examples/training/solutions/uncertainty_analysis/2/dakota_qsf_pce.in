environment
  tabular_data

method
    polynomial_chaos
     sparse_grid_level 3
     samples_on_emulator 5e5
     response_levels 0.11 0.3
     seed = 12347 

variables
  uniform_uncertain 2
   descriptors 'x1' 'x2'
   lower_bounds = -0.8 -0.8
   upper_bounds =  0.8  0.8

#  triangular_uncertain 2
#   descriptors 'x1' 'x2'
#   lower_bounds = -0.8 -0.8
#   upper_bounds =  0.8  0.8
#   modes        =  0.0  0.0

responses
  response_functions 1
  analytic_gradients
  no_hessians

interface
  analysis_drivers 'quasi_sine_fcn'
    fork 
  asynchronous evaluation_concurrency 4
