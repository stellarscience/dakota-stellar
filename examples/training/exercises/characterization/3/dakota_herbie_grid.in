environment
  tabular_data
    tabular_data_file = 'herbie_grid_tabular.dat'

method
  multidim_parameter_study
    partitions = 10

variables
  continuous_design = 2
    initial_point   =  0.9 -1.1
    lower_bounds    = -2.0 -2.0
    upper_bounds    =  2.0   2.0
    descriptors     = 'x1' 'x2'

interface
  analysis_drivers = 'herbie'
    direct

responses
  response_functions = 1
  descriptors      = 'f1'	
  no_gradients
  no_hessians
